{
  "name": "librelibre",
  "version": "0.0.0-see-git-tags",
  "description": "Opinionated CLI for continuous delivery of npm packages",
  "main": "dist/main",
  "bin": {
    "libre": "dist/main.js"
  },
  "repository": "git@github.com:prisma-labs/libre.git",
  "author": "Jason Kuhrt <jasonkuhrt@me.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "/src",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "libre": "ts-node src/main",
    "clean": "rm -rf dist oclif.manifest.json",
    "build": "oclif-dev manifest && tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "build:doc:toc": "doctoc README.md",
    "build:doc:commands": "oclif-dev readme",
    "build:doc": "yarn -s build:doc:commands && yarn -s build:doc:toc",
    "postpublish": "yarn -s run clean",
    "prepublishOnly": "yarn -s run build"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "globby": "^10.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "doctoc": "^1.4.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "libre",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
